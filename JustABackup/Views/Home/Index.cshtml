@model ListJobHistoryModel

<table class="table table-responsive table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Started</th>
            <th>Run time</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var job in Model.JobHistory)
        {
            <tr>
                <td>@job.JobName</td>
                <td>@job.Started</td>
                <td>@job.RunTime</td>
                <td>@job.Status</td>
            </tr>
        }
    </tbody>
</table>