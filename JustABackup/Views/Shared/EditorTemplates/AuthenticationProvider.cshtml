@model JustABackup.Base.IAuthenticatedClient<object>
@inject IAuthenticatedSessionRepository authenticatedSessionRepo
@{ 
    var sessions = await authenticatedSessionRepo.GetAuthenticatedSessions(ViewData["GenericParameter"] as string);
    var selectItems = sessions.Select(kvp => new SelectListItem { Value = kvp.Key.ToString(), Text = kvp.Value });
}

<select asp-for="@Model" asp-items="selectItems" class="form-control"></select>